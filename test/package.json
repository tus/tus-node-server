    "@types/node": "^20.5.7",
    "@types/rimraf": "^3.0.2",
    "@types/sinon": "^10.0.16",
    "@types/supertest": "^2.0.12",
    "mocha": "^10.2.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "typescript": "^5.2.2"
  }
