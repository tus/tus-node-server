{
  "name": "tus-node-server",
  "description": "Node.js 4 ES2015 tus server, including a transpiled down version for older &lt; 1.0 versions",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tus/tus-node-server.git"
  },
  "keywords": [
    "tus",
    "framework",
    "resumable",
    "upload",
    "protocol",
    "app",
    "api",
    "server"
  ],
  "author": "Ben Stahl <bhstahl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tus/tus-node-server/issues"
  },
  "homepage": "https://github.com/tus/tus-node-server#readme",
  "engines": {
    "node": ">= 4.0.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib/"
  ],
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha",
    "coverage": "NODE_ENV=test istanbul cover ./node_modules/mocha/bin/_mocha && open coverage/lcov-report/index.html",
    "coveralls": "NODE_ENV=test istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "example": "cd example && node server.js",
    "lint": "eslint ."
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-eslint": "^4.1.3",
    "coveralls": "^2.11.4",
    "eslint": "^1.7.3",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "should": "^7.1.1",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "crypto-rand": "0.0.2"
  }
}
